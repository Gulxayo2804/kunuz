                                                    </tr>
                                                </thead>
                                                <% for(item of data) {%>
                                                <tbody> 
                                                    <tr>
                                                        <td><%= item.name%></td>
                                                        <td><%= item.createdAt%></td>
    
                                                        <td>
                                                            <div>
                                                                <a href= "/category/add" class="btn btn-primary btn-sm">Create</a>
                                                            </div>
                                                        </td>
                                                        <td>
                                                            <div>
                                                                <a href= "/category/all/<%= `${item._id}`%>" class="btn btn-primary btn-sm">Edit</a>
                                                            </div>
                                                        </td>
                                                        <td>
                                                            <form method="POST"  action="<%=`/category/delete/${item._id}?_method=DELETE`%>" >
                                                            <div>
                                                                <a><button class="btn btn-primary btn-sm"> Delete</button></a>
                                                            </div>
                                                        </form>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                                <% } %>
                                            </table>
                                            
                                        </div>
    
                                    </div>
                                </div>
                            </div>
    
                        </div>

                        <!-- END ROW -->
                    </div>
                    <!-- container-fluid -->

                </div>
